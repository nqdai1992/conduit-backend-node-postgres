    "@types/pg": "^7.14.11",
    "@types/pg-copy-streams": "^1.2.1",
    "@types/sinon": "^10.0.4",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "~4.16.1",
    "@typescript-eslint/parser": "~4.16.1",
    "chai": "^4.3.4",
